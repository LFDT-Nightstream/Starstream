{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "npm run build-vsix && npm run build-sandbox && docusaurus build",
    "build-vsix": "cd ../vscode-starstream && npm i && npx vsce package -o starstream.vsix",
    "build-sandbox": "cargo build -p starstream-sandbox-web --target wasm32-unknown-unknown --release",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@typefox/monaco-editor-react": "^7.2.0",
    "@zip.js/zip.js": "^2.8.8",
    "cbor2": "^2.0.1",
    "docusaurus-plugin-generate-llms-txt": "^0.0.1",
    "fancy-ansi": "^0.1.3",
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@^22.1.3",
    "monaco-languageclient": "^10.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
