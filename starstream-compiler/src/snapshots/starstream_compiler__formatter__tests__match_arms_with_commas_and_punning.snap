struct Point {
    x: i64,
}

enum Message {
    Ping,
    Pong {
        x: i64,
    },
}

fn add(a: Point, b: Message) -> i64 {
    match b {
        Message::Ping => {
            a.x
        },
        Message::Pong { x } => {
            x + a.x
        },
    }
}
